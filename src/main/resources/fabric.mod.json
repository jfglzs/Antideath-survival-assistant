{
	"schemaVersion": 1,
	"id": "asa",
	"version": "${version}",
	"name": "ad-sur-assistant",
	"description": "masa extension mod that provides some useful features for your game.",
	"authors": [
		"antideath"
	],
	"contact": {
		"homepage": "https://github.com/jfglzs/Antideath-survival-assistant/",
		"sources": "https://github.com/jfglzs/Antideath-survival-assistant"
	},
	"license": "CC0-1.0",
	"icon": "assets/asa/icon.png",
	"environment": "*",
	"entrypoints": {
        "client": [
          "io.github.jfglzs.AsaMod"
        ],
        "modmenu": [
          "io.github.jfglzs.config.ModMenu"
        ]
	},
	"mixins": [
		"ad-sur-assistant.mixins.json"
	],
    "accessWidener": "asa.accesswidener",
	"depends": {
		"fabricloader": "*",
        "malilib": "${malilib_requirement_version}",
        "minecraft": "${minecraft_requirement_version}",
        "java": ">=21",
        "fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}